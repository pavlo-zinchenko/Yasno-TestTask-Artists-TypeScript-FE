{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": true,
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "allowImportingTsExtensions": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": [
        "src/assets/*"
      ],
      "@common/*": [
        "src/components/common/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@slices/*": [
        "src/store/slices/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@validations/*": [
        "src/validations/*"
      ]
    }
  }
}